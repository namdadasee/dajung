<?xml version="1.0" encoding="UTF-8"?>
<Context>
	<Resource
		name="jdbc/MySQLDB"
		auth="Container"
		type="javax.sql.DataSource"
		username="java"
		password="java"
		driverClassName="com.mysql.jdbc.OracleDriver"
		factory="org.apache.tomcat.dbcp.dbcp2.BasicDataSourceFactory"
		url="jdbc:oracle:thin:@localHost:1521:XE"
		maxActive="500"
		maxIdle="100"/>
</Context>